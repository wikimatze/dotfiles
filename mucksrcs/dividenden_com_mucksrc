#
# dividenden-com
#
mux: tmux
name: dividenden-com
dir: ~/Downloads/pdf-reader/examples


[dividenden-com]
tmux send-keys -tdividenden-com:1.1 'ruby dividenden.rb com' Enter
- sh sleep 3.5

[dividenden-com-rules]
nvim dividenden_rules.rb

[finanzen]
nvim ~/nextcloud/notes/finanzen.txt

[nautilus]
nautilus ~/Downloads/matze_comdirect
- sh sleep 5.5

[bla]

[tmp]
tmux send-keys -tdividenden-com:5.1 '' Enter
- sh sleep 3.5
tmux send-keys -tdividenden-com:5.1 'nautab.sh ~/Downloads/pdf-reader/examples/com' Enter
- sh sleep 3.5
tmux send-keys -tdividenden-com:5.1 'nautab.sh ~/nextcloud/finanzen/matze' Enter
- sh sleep 3.5
tmux select-window -tdividenden-com
- sh sleep 3.5
tmux kill-window -t bla
tmux kill-window -t tmp
